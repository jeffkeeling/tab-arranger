{
  "manifest_version": 3,
  "name": "Tab Arranger",
  "version": "1.0",
  "browser_specific_settings": {
    "gecko_android": {},
    "gecko": {
      "id": "{14f242d6-4f0e-4346-955e-0f78d22b7212}"
    }
  },
  "description": "Arrange all your tabs by clicking toolbar button that opens a popup listing all the tabs",
  "permissions": ["tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  }
}
